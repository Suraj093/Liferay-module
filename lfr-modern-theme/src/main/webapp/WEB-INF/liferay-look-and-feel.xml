<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 7.4.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_7_4_0.dtd">

<look-and-feel>
	<compatibility>
		<version>7.0.0+</version>
		<version>7.1.0+</version>
		<version>7.2.0+</version>
		<version>7.3.0+</version>
		<version>7.4.0+</version>
	</compatibility>
	<theme id="lfr-modern-theme" name="lfr-modern-theme">
		<template-extension>ftl</template-extension>
		<settings>
			<setting configurable="true" key="show-header"
				type="checkbox" value="true"></setting>
				<setting configurable="true" key="show-footer" type="checkbox" value="true"></setting>
		</settings>
	</theme>
</look-and-feel>